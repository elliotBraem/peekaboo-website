body,
html
  overflow-x: hidden
  font-family: 'Gill Sans Book', Helvetica, sans-serif

body h1,
body h2,
html h1,
html h2
  color: #5E239D
  font-family: 'Marker-Felt'

img
  user-drag: none
  user-select: none
  -moz-user-select: none
  -webkit-user-drag: none
  -webkit-user-select: none
  -ms-user-select: none

nav a
  color: #3B5998

section
  padding-bottom: 120px

ul
  list-style-type: none

.logo
  margin: 50px 0 80px 0
  max-width: 500px

.child
  max-height: 200px
  max-width: 250px

.blue-box
  background-color: #D6EFFF
  margin: 10px 10px 50px 10px
  -webkit-box-shadow: 9px 6px 19px 0px rgba(0, 0, 0, 0.3)
  -moz-box-shadow: 9px 6px 19px 0px rgba(0, 0, 0, 0.3)
  box-shadow: 9px 6px 19px 0px rgba(0, 0, 0, 0.3)
  padding: 5px


// Devices smaller than 350px
@media only screen and (max-width: 350px)
  .blue-box
    margin: 0px
    -webkit-box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0)
    -moz-box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0)
    box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0)


